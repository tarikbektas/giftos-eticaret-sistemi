      <input type="hidden" name="productName" value="test ürün 2" >
      <input type="hidden" name="price"   value="1234">
      <button type="submit">kart ekle 2</button>
     </form>
     <form action="urun-ekle" method="post">
               
      <input type="hidden" name="productName" value="test ürün 3" >
      <input type="hidden" name="price"   value="1234">
      <button type="submit">kart ekle 3</button>
     </form>    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav">
        <% navbar.forEach(function(navbaritems) { %>
        <li class="nav-item">
          <% if (navbaritems.url) { %>
          <a class="nav-link" href="<%=navbaritems.url%>"><%=navbaritems.title%> <span class="sr-only">(current)</span></a>
          <% } else if (navbaritems.url2) { %>
          <a class="nav-link" href="/<%=navbaritems.url2%>"><%=navbaritems.title%> <span class="sr-only">(current)</span></a>
          <% } else { %>
          <!-- Eğer her ikisi de boşsa istediğiniz başka bir şey yapabilirsiniz -->
          <span class="nav-link"><%=navbaritems.title%></span>
          <% } %>
        </li>
        <% }); %>
      </ul>
      <%if(!user){ %>
      <div class="user_option">
        <a href="/login">
          <i class="fa fa-user" aria-hidden="true"></i>
          <span>
            Login
          </span>
        </a>
        <a href="">
          <i class="fa fa-shopping-bag" aria-hidden="true"></i>
        </a>
        <form class="form-inline ">
          <button class="btn nav_search-btn" type="submit">
            <i class="fa fa-search" aria-hidden="true"></i>
          </button>
        </form>
      </div>
      <% } %>
      <%if(user){ %>

      <div class="pull-right">
        <ul class="nav pull-right">
          <li class="dropdown"><a href="#" class="dropdown-toggle" data-toggle="dropdown">Welcome, <%-user.name  %> <b class="caret"></b></a>
            <ul class="dropdown-menu">
              <li>
                <form action="/logout" method="POST">
                  <button class="btn btn-primary btn-sm" type="submit">çıkış </button>
                </form>
              </li>
            </ul>
          </li>
        </ul>
      </div>
      <% } %>
